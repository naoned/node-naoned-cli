{
	"config": {
		"commitizen": {
			"path": "./node_modules/naoned-cz-conventional-changelog"
		},
		"ghooks": {
			"commit-msg": "naoned commit-msg",
			"pre-commit": "naoned pre-commit"
		},
		"validate-commit-msg": {
			"types": ["feat", "fix", "docs", "style", "refact", "perf", "test", "misc", "revert"],
			"warnOnFail": false,
			"maxSubjectLength": 100
		}
	}
}